
qsort li = case li of
  { Nil -> Nil
  ; Cons x xs -> 
       let { less y = y < x
           ; more y = y >= x
           ; listLess = filter less xs
           ; listMore = filter more xs
           } 
       in append (qsort listLess) (Cons x (qsort listMore))
  };
  
inc x = x + 1;

main = qsort (reverse (take 10 (iterate inc 0)));

