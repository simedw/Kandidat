

qsort li = case li of
  { Nil -> Nil
  ; Cons x xs -> 
       let { lesser y = y < x
           ; listLess = filter lesser xs
           ; listMore = filter (\y . y >= x) xs
           } 
       in append (qsort listLess) (Cons x (qsort listMore))
  };
  

inc x = x + 1;


main = qsort (reverse (take 3 (iterate inc 0)));

